type days_of_week=(sat,sun,mon,tue,wed,thu,fri);

     myinteger=integer;

     ashkan=record
     name:string;
     number:integer;
     c:char;
     end;


var day:days_of_week;
    i  :integer;
    j  :myinteger;
    ashkan1:ashkan;
begin
     ashkan1.name:='amin';
     day:=sun;
     i  :=10 ;
     j  :=-2;
end.